{
  "name": "portfolio-in-netlify",
  "version": "1.0.0",
  "description": "Professional",
  "type": "module",
  "scripts": {
    "build:js": "terser script.js -c -m -o dist/script.min.js",
    "build:css": "cleancss -o dist/style.min.css style.css",
    "build:html": "html-minifier-terser --collapse-whitespace --remove-comments --minify-css true --minify-js true index.html -o dist/index.html",
    "prepare:dist": "node scripts/prepare-dist.js",
    "build": "npm run prepare:dist && npm run build:js && npm run build:css && npm run build:html",
    "postbuild": "node scripts/patch-html.js",
    "serve": "npx netlify dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "html-minifier-terser": "^7.2.0",
    "terser": "^5.44.1"
  }
}
